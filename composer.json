{
    "name": "phlak/laravel-pivot",
    "description": "This package introduces new eloquent events for sync(), attach(), detach() or updateExistingPivot() methods on BelongsToMany relation.",
    "keywords": [
        "laravel BelongsToMany events",
        "eloquent extra events",
        "laravel pivot events",
        "laravel sync events",
        "eloquent events"
    ],
    "homepage": "https://github.com/phlak/laravel-pivot",
    "support": {
        "issues": "https://github.com/phlak/laravel-pivot/issues",
        "source": "https://github.com/phlak/laravel-pivot"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Filip Horvat",
            "email": "filip.horvat@am2studio.hr",
            "homepage": "http://am2studio.hr",
            "role": "Developer"
        },
        {
            "name": "Chris Kankiewicz",
            "email": "Chris@ChrisKankiewicz.com",
            "homepage": "https://www.chriskankiewicz.com"
        }
    ],
    "require": {
        "illuminate/database": "^5.5|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "friendsofphp/php-cs-fixer": "*"
    },
    "autoload": {
        "psr-4": {
            "PHLAK\\Laravel\\Pivot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHLAK\\Laravel\\Pivot\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
